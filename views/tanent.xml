<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<!-- Action  -->
	 	<record id="property_tax_calculator.action_all_tanent" model="ir.actions.act_window">
            <field name="name">All Tanent</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">res.partner</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{'default_tenant':1}</field>
            <field name="domain" eval="[('tenant', '=', True)]"/>
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">
                Click to add a tanent in your database.
              </p><p>
                We helps you easily track all activities related to
                a Tanent: Building ,Accounts
                documents, etc.
              </p>
            </field>
        </record>
        <record id="property_tax_calculator.action_all_tanent_tree" model="ir.actions.act_window.view">
            <field eval="1" name="sequence"/>
            <field name="view_mode">tree</field>
            <field name="act_window_id" ref="property_tax_calculator.action_all_tanent"/>
        </record>

        <record id="property_tax_calculator.action_all_tanent_form" model="ir.actions.act_window.view">
            <field eval="2" name="sequence"/>
            <field name="view_mode">form</field>
            <field name="view_id" ref="property_tax_calculator.view_partner_form"/>
            <field name="act_window_id" ref="property_tax_calculator.action_all_tanent"/>
        </record>

	<!-- Menuitem -->
	    <menuitem 
			name="Tatent" 
			id="property_tax_calculator.tatent" 
			parent="property_tax_calculator.menu_1"
			sequence='3'
			action="property_tax_calculator.action_all_tanent"
	    />
</odoo>